<?xml version="1.0" encoding="UTF-8"?>

<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<application>
	    <el-resolver>
	org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

	<managed-bean>
	    <managed-bean-name>deployProcess</managed-bean-name>
	    <managed-bean-class>com.fy.viewer.DeployProcess</managed-bean-class>
	    <managed-bean-scope>request</managed-bean-scope>
	    <managed-property>
		     <property-name>jBPMUtil</property-name>
		     <value>#{jBPMUtil}</value>
		</managed-property>
	</managed-bean>
  	
  	<navigation-rule>
  	 <navigation-case> 
      	<from-action>#{login.loginAction}</from-action>
        <from-outcome>admin</from-outcome> 
        <to-view-id>/manager.faces</to-view-id> 
     </navigation-case>
     <navigation-case> 
      	<from-action>#{login.loginAction}</from-action>
        <from-outcome>manager</from-outcome> 
        <to-view-id>/menu.faces</to-view-id> 
      </navigation-case>
      <navigation-case> 
      	<from-action>#{login.loginAction}</from-action>
        <from-outcome>shouli</from-outcome> 
        <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
      <navigation-case> 
      	<from-action>#{login.loginAction}</from-action>
        <from-outcome>kancha</from-outcome> 
        <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
      <navigation-case> 
      	<from-action>#{login.loginAction}</from-action>
        <from-outcome>pinggu</from-outcome> 
        <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
      <navigation-case> 
      	<from-action>#{login.loginAction}</from-action>
        <from-outcome>shenpi</from-outcome> 
        <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
      <navigation-case> 
      	<from-action>#{login.loginAction}</from-action>
        <from-outcome>worker</from-outcome> 
        <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
    <from-view-id>/browers.xhtml</from-view-id>
  	 <navigation-case>
          <from-outcome>projectAdd</from-outcome> 
          <to-view-id>/projectAdd.faces</to-view-id> 
      </navigation-case>
     <navigation-case>
          <from-outcome>menu</from-outcome> 
          <to-view-id>/menu.faces</to-view-id> 
      </navigation-case> 
    </navigation-rule>
    
    <navigation-rule>
    <from-view-id>/kanchaProject.xhtml</from-view-id>
  	 <navigation-case>
          <from-outcome>kancha</from-outcome> 
          <to-view-id>/projectAdd.faces</to-view-id> 
     </navigation-case>
     <navigation-case>
          <from-outcome>menu</from-outcome> 
          <to-view-id>/menu.faces</to-view-id> 
      </navigation-case> 
    </navigation-rule>

    <navigation-rule>
    <from-view-id>/projectAdd.xhtml</from-view-id>
  	 <navigation-case>
          <from-outcome>kancha</from-outcome> 
          <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
      <navigation-case>
          <from-outcome>pinggu</from-outcome> 
          <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
      <navigation-case>
          <from-outcome>shencha</from-outcome> 
          <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
      <navigation-case>
          <from-outcome>shenpi</from-outcome> 
          <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
      
    </navigation-rule>
    
    <navigation-rule>
    <from-view-id>/userchange.xhtml</from-view-id>
  	 <navigation-case>
          <from-outcome>success</from-outcome> 
          <to-view-id>/kanchaProject.faces</to-view-id> 
      </navigation-case>
    </navigation-rule>
    
    <!-- JSF BEAN 声明 -->

    
	<managed-bean>
	    <managed-bean-name>login</managed-bean-name>
	    <managed-bean-class>
	         com.fy.viewer.loginVeryfy
	    </managed-bean-class>
	    <managed-bean-scope>session</managed-bean-scope>
	    <managed-property>
		     <property-name>userService</property-name>
		     <value>#{userService}</value>
		   </managed-property>
	</managed-bean>
	
	<managed-bean>
	    <managed-bean-name>usermanager</managed-bean-name>
	    <managed-bean-class>
	         com.fy.viewer.UserManager
	    </managed-bean-class>
	    <managed-bean-scope>request</managed-bean-scope>
	    <managed-property>
		     <property-name>userService</property-name>
		     <value>#{userService}</value>
		   </managed-property>
		<managed-property>
		     <property-name>jBPMUtil</property-name>
		     <value>#{jBPMUtil}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
	    <managed-bean-name>userchange</managed-bean-name>
	    <managed-bean-class>
	         com.fy.viewer.UserChange
	    </managed-bean-class>
	    <managed-bean-scope>request</managed-bean-scope>
	    <managed-property>
		     <property-name>userService</property-name>
		     <value>#{userService}</value>
		   </managed-property>
	</managed-bean>
	
	<managed-bean>
	    <managed-bean-name>ProjectEdit</managed-bean-name>
	    <managed-bean-class>
	         com.fy.viewer.ProjectAdd
	    </managed-bean-class>
	    <managed-bean-scope>view</managed-bean-scope>
	    <managed-property>
		     <property-name>projectFileService</property-name>
		     <value>#{projectFileService}</value>
		</managed-property>
	    <managed-property>
		     <property-name>projectService</property-name>
		     <value>#{projectService}</value>
		</managed-property>
		<managed-property>
		     <property-name>jBPMUtil</property-name>
		     <value>#{jBPMUtil}</value>
		</managed-property>
		<managed-property>
		     <property-name>userService</property-name>
		     <value>#{userService}</value>
		</managed-property>
		<managed-property>
		     <property-name>projectSeqService</property-name>
		     <value>#{projectSeqService}</value>
		</managed-property>
		<managed-property>
		     <property-name>marketCompareService</property-name>
		     <value>#{marketCompareService}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
	    <managed-bean-name>ProjectBrowse</managed-bean-name>
	    <managed-bean-class>
	         com.fy.viewer.ProjectBrowse
	    </managed-bean-class>
	    <managed-bean-scope>view</managed-bean-scope>
		<managed-property>
		     <property-name>projectService</property-name>
		     <value>#{projectService}</value>
		</managed-property>
		<managed-property>
		     <property-name>jBPMUtil</property-name>
		     <value>#{jBPMUtil}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
	    <managed-bean-name>ProjectTrace</managed-bean-name>
	    <managed-bean-class>
	         com.fy.viewer.ProjectTrace
	    </managed-bean-class>
	    <managed-bean-scope>request</managed-bean-scope>
		<managed-property>
		     <property-name>jBPMUtil</property-name>
		     <value>#{jBPMUtil}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
	    <managed-bean-name>KanchaProject</managed-bean-name>
	    <managed-bean-class>
	         com.fy.viewer.KanchaProject
	    </managed-bean-class>
	    <managed-bean-scope>view</managed-bean-scope>
		<managed-property>
		     <property-name>projectService</property-name>
		     <value>#{projectService}</value>
		</managed-property>
		<managed-property>
		     <property-name>jBPMUtil</property-name>
		     <value>#{jBPMUtil}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
	    <managed-bean-name>PingguProject</managed-bean-name>
	    <managed-bean-class>
	         com.fy.viewer.PingguProject
	    </managed-bean-class>
	    <managed-bean-scope>view</managed-bean-scope>
		<managed-property>
		     <property-name>projectService</property-name>
		     <value>#{projectService}</value>
		</managed-property>
		<managed-property>
		     <property-name>jBPMUtil</property-name>
		     <value>#{jBPMUtil}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
	    <managed-bean-name>ProjectStatistics</managed-bean-name>
	    <managed-bean-class>
	         com.fy.viewer.ProjectStatistics
	    </managed-bean-class>
	    <managed-bean-scope>view</managed-bean-scope>
	    <managed-property>
		     <property-name>projectService</property-name>
		     <value>#{projectService}</value>
		</managed-property>
		<managed-property>
		     <property-name>jBPMUtil</property-name>
		     <value>#{jBPMUtil}</value>
		</managed-property>
		<managed-property>
		     <property-name>userService</property-name>
		     <value>#{userService}</value>
		</managed-property>
		
	</managed-bean>
	
</faces-config>
